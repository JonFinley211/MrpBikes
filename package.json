  
{
  "name": "bike-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client2-install": "npm install --prefix client2",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node index.js",
    "start:dev": "concurrently \"nodemon --ignore 'client2/*'\" \"npm run client2\"",
    "server": "nodemon index.js",
    "client2": "cd client2 && npm run start",
    "dev": "concurrently \"npm run server\" \"npm run client2\"",
    "instalx": "cd client2 && npm install",
    "builx": "cd client2 && npm run build",
    "heroku-postbuild":  "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
  },
  "author": "Jon",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  }
}